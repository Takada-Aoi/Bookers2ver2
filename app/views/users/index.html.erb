<%= render 'users/info',user:@user %>
<%= render 'books/new',book:@book %>

<table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tboby>
    <% @users.each do |user| %>
    <tr>
      <td><%=attachment_image_tag user, :profile_image,:fill,40,40 %></td>
      <td><%=user.name%></td>
      <td><%=link_to "Show",user_path(user.id)%></td>
    </tr>
    <% end %>
  </tboby>
</table>

