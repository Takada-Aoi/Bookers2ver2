<%= render 'users/info',user:@user %>

<%= render 'books/new',book:@book %>

<table>
  <thead>
    <tr>
      <th>title</th>
      <th>body</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tboby>
    <% @books.each do |book| %>
    <tr>
      <td><%=book.title %></td>
      <td><%=book.body%></td>
      <td><%=link_to "Show",book_path(book.id)%></td>
    </tr>
    <% end %>
  </tboby>
</table>